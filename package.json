{
  "name": "express-sandbox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --env-file=.env dist/server.js",
    "prestart": "npm run -s build",
    "build": "npm-run-all -p --silent build:*",
    "build:ts": "npm run -s build:ts:compile && npm run -s build:ts:bundleDeps",
    "build:ts:compile": "tsc --project src/frontend/ && tsc --project ./",
    "build:ts:bundleDeps": "rollup -c",
    "build:style": "sass src/frontend/stylesheets/style.scss dist/public/styles/style.css -s compressed --no-source-map",
    "build:views": "node -e \"require('fs').cpSync('./src/frontend/views', './dist/views', {recursive: true});\"",
    "build:module": "node -e \"require('fs').cpSync('./src/dist.package.json', './dist/package.json');\""
  },
  "type": "module",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "http-errors": "~1.6.3",
    "memorystore": "^1.6.7",
    "morgan": "~1.9.1",
    "nunjucks": "^3.2.4",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.28",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.16",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.13.0",
    "sass": "^1.72.0",
    "typescript": "^5.4.2"
  }
}
